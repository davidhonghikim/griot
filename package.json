{
  "name": "@griot/framework",
  "version": "1.0.0",
  "description": "Complete AI agent framework for building sophisticated agent systems",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "griot": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx src/cli/dev.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "start": "node dist/index.js"
  },
  "keywords": [
    "ai",
    "agents",
    "framework",
    "automation",
    "messaging",
    "microservices",
    "typescript"
  ],
  "author": "Griot Framework Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fastify": "^4.25.2",
    "inquirer": "^9.2.12",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.4",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.35.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/griot-framework/griot-node.git"
