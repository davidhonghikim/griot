# AI-Q System Environment Configuration
# Copy this file to .env and fill in your actual values

# ==== LLM SERVICE API KEYS ====
OPENAI_API_KEY=your_openai_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# ==== EXTERNAL MCP SERVICES ====
BROWSERBASE_API_KEY=your_browserbase_api_key_here
SUPABASE_ACCESS_TOKEN=your_supabase_access_token_here
SUPABASE_PROJECT_REF=your_supabase_project_ref_here

# ==== DATABASE CREDENTIALS ====
POSTGRES_PASSWORD=secure_postgres_password_change_me
GRAFANA_PASSWORD=secure_grafana_password_change_me

# ==== GITHUB INTEGRATION (Optional) ====
GITHUB_TOKEN=your_github_personal_access_token_here

# ==== MCP GATEWAY CONFIGURATION ====
GATEWAY_JWT_SECRET=your_jwt_secret_key_change_me_to_long_random_string
GATEWAY_LOG_LEVEL=info
GATEWAY_MAX_REQUESTS_PER_MINUTE=60

# ==== SECURITY SETTINGS ====
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:80
CORS_ENABLED=true
RATE_LIMITING_ENABLED=true
